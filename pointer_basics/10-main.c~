#include <stdio.h>
int main(void)
{
  int a[5];
  *a = 98;
  *(a + 1) = 198;
  *(a + 2) = 298;
  *(a + 3) = 398;
  printf("Value of a[0]: %d\n", *a);
  printf("Value of a[1]: %d\n", *(a + 1));
  printf("Value of a(Address of a[1]): %p\n", a);
  printf("Address of a[1]: %p\n", (a + 1));
  printf("Address of a[1]: %p\n", &(*(a + 1)));
  return(0);
}
